tech_stock_tickers <- c("MSFT", "AAPL", "GOOGL", "FB", "INTC", "CSCO", "TSM", "ORCL", "ADBE", "IBM")
analysis_dates <- list(start = as.Date("2016-01-01"), end = as.Date(Sys.Date()))
gaming_stocks_raw <-
